    "typescript": "^3.3.3333"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hfour/promise-observer.git"
  },
  "bugs": {
    "url": "https://github.com/hfour/promise-observer/issues"
  },
  "homepage": "https://github.com/hfour/promise-observer",
  "jest": {
    "preset": "ts-jest",
    "testPathIgnorePatterns": [
